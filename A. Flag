#include <bits/stdc++.h>
using namespace std;
 
#define fio() ios_base::sync_with_stdio(false);\
cin.tie(NULL);
 
#define md 998244353 
typedef long long int lli;
typedef float fl;
#define mem(arr,x) memset(arr,x,sizeof(arr))


std::vector <int> x;
std::vector <int> y;

map <int, int> xx;
map <int, int> yy;

std::vector <int> :: iterator vit;
map <int,int> :: iterator mit;

typedef struct tim
            {
               lli a; 
               lli pos;
            }an;


lli comparator(an p,an q) 
{ 
    return (   p.a < q.a ) ; 
} 

lli power(lli x, lli y, lli p) 
{ 
    lli res = 1;    
    x = x % p;  
    while (y > 0) 
    { 
        if (y & 1) 
            res = (res*x) % p; 
  
        y = y>>1; 
        x = (x*x) % p; 
    } 
    return res; 
}
 
 
lli modInverse(lli n, lli p) 
{ 
    return power(n, p-2, p); 
}

 

int main()
{    
    fio();
    lli n,m,i,j,k,l;
    cin>>n>>m;
    string s[n];
    for(i=0;i<n;i++)
      cin>>s[i];
    
    k=1;
    for(i=0;i<n;i++)
    {
      if(s[i][0]==s[i-1][0] && i) k=0;
      for(j=0;j<m-1;j++)
      {
        if(s[i][j]!=s[i][j+1]) 
        k=0;
      }
      if(k==0) break;
    }
    
    if(k) cout<<"YES";
    else cout<<"NO";
     
}
